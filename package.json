  },
  "scripts": {
    "dev": "node install.js && cross-env DEBUG=socket.io:* nodemon app.js",
    "start": "ENVIRONMENT=production node app.js",
    "postinstall": "node install.js",
    "premocha": "node install.js",
    "mocha": "mocha test/backend",
    "karma": "karma start --single-run",
    "test": "npm run mocha && npm run karma",
    "testrun": "MONGO_AGGIE_DB=aggie-test node app.js",
    "preprotractor": "webdriver-manager update",
    "build": "CI=false react-scripts build",
    "server front-end": "react-scripts test",
    "protractor": "protractor test/end-to-end/protractor.conf.js",
    "protractor-with-apis": "protractor test/end-to-end/with-apis/protractor.conf.js",
    "update-ct-lists": "node backend/scripts/ct-list-update.js",
    "start dev frontend": "PORT=8000 react-scripts start",
    "start dev backend": "ENVIRONMENT=development nodemon app.js",
    "dev:all": "stmux -- [ \"npm run 'start dev frontend'\" .. \"npm run 'start dev backend'\" ]",
    "test frontend": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "subdomain": "aggie",
  "engines": {
    "node": "^16.13.0",
    "npm": "^8.1.4"
  },
  "nodemonConfig": {
    "ignore": [
      "test/*",
      "docs/*"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
